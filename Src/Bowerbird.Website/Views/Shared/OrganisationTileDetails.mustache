<h3>
  <a href="/{{Id}}">{{Name}}</a>
</h3>
<div class="avatar">
  {{#Avatar}}
  {{>MediaSquare200}}
  {{/Avatar}}
</div>
<p class="description">
  {{Description}}
</p>
<ul class="categories">
  {{#Categories}}
  <li>
    <li class="small-category-icon category-{{.}}">
      {{.}}
    </li>
  </li>
  {{/Categories}}
</ul>
<div class="info">
  <div class="sighting-actions">
    <div class="users-badge small">
      <span class="small-icon user-icon"></span>
      <span class="count">{{UserCount}}</span>
      <span class="label">members</span>
    </div>
  </div>
</div>
<div class="actions"></div>